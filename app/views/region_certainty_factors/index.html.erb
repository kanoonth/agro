<p id="notice"><%= notice %></p>

<h1>Listing Region Certainty Factors</h1>

<table>
  <thead>
    <tr>
      <th>Region</th>
      <th>Disease</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @region_certainty_factors.each do |region_certainty_factor| %>
      <tr>
        <td><%= region_certainty_factor.region %></td>
        <td><%= region_certainty_factor.disease %></td>
        <td><%= link_to 'Show', region_certainty_factor %></td>
        <td><%= link_to 'Edit', edit_region_certainty_factor_path(region_certainty_factor) %></td>
        <td><%= link_to 'Destroy', region_certainty_factor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Region certainty factor', new_region_certainty_factor_path %>
