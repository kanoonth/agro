<p id="notice"><%= notice %></p>

<h1>Listing Plantation Certainty Factors</h1>

<table>
  <thead>
    <tr>
      <th>Disease</th>
      <th>Plantation</th>
      <th>Cf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plantation_certainty_factors.each do |plantation_certainty_factor| %>
      <tr>
        <td><%= plantation_certainty_factor.disease %></td>
        <td><%= plantation_certainty_factor.plantation %></td>
        <td><%= plantation_certainty_factor.cf %></td>
        <td><%= link_to 'Show', plantation_certainty_factor %></td>
        <td><%= link_to 'Edit', edit_plantation_certainty_factor_path(plantation_certainty_factor) %></td>
        <td><%= link_to 'Destroy', plantation_certainty_factor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plantation certainty factor', new_plantation_certainty_factor_path %>
