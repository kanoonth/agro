<p id="notice"><%= notice %></p>

<h1>Listing Winds</h1>

<table>
  <thead>
    <tr>
      <th>Min</th>
      <th>Max</th>
      <th>Disease</th>
      <th>Cf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @winds.each do |wind| %>
      <tr>
        <td><%= wind.min %></td>
        <td><%= wind.max %></td>
        <td><%= wind.disease %></td>
        <td><%= wind.cf %></td>
        <td><%= link_to 'Show', wind %></td>
        <td><%= link_to 'Edit', edit_wind_path(wind) %></td>
        <td><%= link_to 'Destroy', wind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wind', new_wind_path %>
