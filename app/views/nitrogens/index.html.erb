<p id="notice"><%= notice %></p>

<h1>Listing Nitrogens</h1>

<table>
  <thead>
    <tr>
      <th>Min</th>
      <th>Max</th>
      <th>Stage</th>
      <th>Disease</th>
      <th>Cf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nitrogens.each do |nitrogen| %>
      <tr>
        <td><%= nitrogen.min %></td>
        <td><%= nitrogen.max %></td>
        <td><%= nitrogen.stage %></td>
        <td><%= nitrogen.disease %></td>
        <td><%= nitrogen.cf %></td>
        <td><%= link_to 'Show', nitrogen %></td>
        <td><%= link_to 'Edit', edit_nitrogen_path(nitrogen) %></td>
        <td><%= link_to 'Destroy', nitrogen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nitrogen', new_nitrogen_path %>
